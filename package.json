    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "stage": "yarn build; yarn start",
    "type-check": "tsc",
    "build:tailwind": "tailwindcss build styles/index.css -o styles/storybook.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "build-storybook && gh-pages -d storybook-static",
    "test": "jest __tests__/.*/*.test.tsx?$",
    "db:dev": "prisma db push --preview-feature",
    "lint": "next lint"
  },
  "engines": {
    "node": ">=14"
  },
